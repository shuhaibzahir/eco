  <script>
   
    

    $(document).ready(function () {
    

      $().ready(function () {
        if({{{checkData}}}){
        $('#datatablesales').DataTable({
        dom: 'Bfrtip',
        buttons: [
            { extend: 'copyHtml5', footer: true ,className: 'btn btn-success ' },
            { extend: 'excelHtml5', footer: true ,className: 'btn btn-success' },
            { extend: 'csvHtml5', footer: true ,className: 'btn btn-success'},
            { extend: 'pdfHtml5', footer: true ,className: 'btn btn-success'},
          ],
      responsive: true,
      language: {
        search: "_INPUT_",
        searchPlaceholder: "Search records",

      },  
      "footerCallback": function (row, data, start, end, display) {                
                //Get data here 
                  var api = this.api(), data;
                //Do whatever you want. Example:
                var totalAmount = 0;
                for (var i = 0; i < data.length; i++) {
                    totalAmount += parseFloat(data[i][4])+parseFloat(data[i][5]);
                }
                 $( api.column(5).footer()).html(totalAmount);
       }
     });
    }
      
      
       $('#datatable').DataTable({
        "pagingType": "full_numbers",
        "lengthMenu": [
          [10, 25, 50, -1],
          [10, 25, 50, "All"]
        ],
        responsive: true,
        language: {
          search: "_INPUT_",
          searchPlaceholder: "Search records",
        }
      });
 });
    });
  </script>
