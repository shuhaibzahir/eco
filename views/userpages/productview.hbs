 
<div class="container">
  <div class="shop-banner">
    <a href="/product-view/{{_id}}"><img src=" /banners/60db1825c7a33d9603db5d06-001.jpg" alt=""></a>
  </div>
  <div class="breadcrumbs style2">
    <a href="#">Home</a>
    <span>Categories_Leftsidebar</span>
  </div>
  <div class="row">
    <div class="main-content col-sm-12">
      <div class="row">
        <div class="col-sm-4">
          <div class="product-detail-image style2">
            <div class="main-image-wapper">
              <img class="main-image imageView" src="/pimages/{{details._id}}-001.jpg"
                data-large="/pimages/{{details._id}}-001.jpg" alt="">
            </div>
            <div class="thumbnails owl-carousel nav-center-center nav-style3"
              data-responsive='{"0":{"items":3},"481":{"items":4},"600":{"items":3},"1000":{"items":4}}'
              data-autoplay="true" data-loop="true" data-items="4" data-dots="false" data-nav="true" data-margin="20">
              <a data-url="/pimages/{{details._id}}-001.jpg" class="active" href="#"><img
                  src="/pimages/{{details._id}}-001.jpg" alt=""></a>
              <a data-url="/pimages/{{details._id}}-002.jpg" href="#"><img src="/pimages/{{details._id}}-002.jpg"
                  alt=""></a>
              <a data-url="/pimages/{{details._id}}-003.jpg" href="#"><img src="/pimages/{{details._id}}-003.jpg"
                  alt=""></a>
              <a data-url="/pimages/{{details._id}}-003.jpg" href="#"><img src="/pimages/{{details._id}}-003.jpg"
                  alt=""></a>
            </div>
          </div>
        </div>
        <div class="col-sm-8">
          <div class="product-details-right style2">
            <h3 class="product-name">{{details.Name}}</h3>

            <span class="price">
              <ins>₹ {{ details.OfferPrize}}</ins>
              <del>₹ {{details.Prize}}</del>
            </span>
            <div class="meta">
              {{#ifEqual details.Quantity 0}}
              <span>Availalbe: <span class="text-primary " style="color: red;"> Outof Stock</span></span>
              {{else}}
              <span>Availalbe: <span class="text-primary "> In Stock</span></span>
              {{/ifEqual }}
            </div>
            <div class="short-descript">
              {{details.Description}}
            </div>
            <div class=" container ">
             <div class="col-md-6">
                <label>Department</label>
              <div class="inner">
                <p>{{details.Department}}</p>
              </div>
              <label>COLOR</label>
              <div class="inner">
                <p>{{details.Color}}</p>
              </div>
             </div>
              <div class="col-md-6">
                <label>Size</label>
              <div class="inner">
                <p>{{details.Size}}</p>
              </div>
              <label>Product Left</label>
              <div class="inner">
                <p>{{details.Quantity}}</p>
              </div>
              </div>
            </div>
            <form id="cartAdding">

           {{#ifEqual details.Quantity 0}}
            <a class="button button-add-cart" data-quantity="1" style="background: red;">Out Of Stock</a>
              {{else}}
               <input  name="productID"  value="{{details._id}}" hidden>
              <input class="button button-add-cart" type="submit" value="Add to Cart">
              {{/ifEqual}}
               </form>
              <a class="wishlist button" href="#" 
                style=" display: flex;
                justify-content: center;
                align-items: center;">
              <i class="fa fa-heart-o" style="font-size: 18px;"></i></a>

           

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
